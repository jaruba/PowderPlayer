{
  "author": {
    "name": "Henri Bouvier"
  },
  "name": "dns",
  "description": "A DNS Server with a REST API",
  "version": "0.2.2",
  "keywords": [
    "dns",
    "REST"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hbouvier/dns.git"
  },
  "dependencies": {
    "winston": "0.7.3",
    "hbo-dnsd": "0.9.8",
    "native-dns": "0.6.1",
    "node-options": "0.0.6",
    "tomahawk": "0.1.6",
    "tomahawk-plugin-kv-memory-store": "0.0.3"
  },
  "engines": {
    "node": ">= 0.10.0 < 0.11.0"
  },
  "main": "./lib/dns.js",
  "bin": {
    "dns": "./bin/dns"
  },
  "scripts": {
    "start": "bin/dns"
  },
  "configuration": {
    "name": "DNS",
    "context": "/",
    "level": "info",
    "port": 8053,
    "plugins": {
      "store": {
        "context": "/dns/api/v1",
        "interval": 1000,
        "implementation": "tomahawk-plugin-kv-memory-store",
        "filename": "${HOME}/.dns/dns-store"
      }
    },
    "dns": {
      "host": "0.0.0.0",
      "port": 53,
      "zone": "local.dev",
      "ttl": 3600,
      "prefix": "dns:",
      "primary": "8.8.8.8",
      "secondary": "8.8.4.4",
      "timeout": 1000
    }
  },
  "bugs": {
    "url": "https://github.com/hbouvier/dns/issues"
  },
  "homepage": "https://github.com/hbouvier/dns",
  "_id": "dns@0.2.2",
  "_shasum": "a8477ca26b835842c3125204d8cbcdd6af98c9ec",
  "_from": "dns@",
  "_npmVersion": "1.4.9",
  "_npmUser": {
    "name": "hbouvier",
    "email": "henribouvier@yahoo.com"
  },
  "maintainers": [
    {
      "name": "hbouvier",
      "email": "henribouvier@yahoo.com"
    }
  ],
  "dist": {
    "shasum": "a8477ca26b835842c3125204d8cbcdd6af98c9ec",
    "tarball": "http://registry.npmjs.org/dns/-/dns-0.2.2.tgz"
  },
  "directories": {},
  "_resolved": "https://registry.npmjs.org/dns/-/dns-0.2.2.tgz"
}
